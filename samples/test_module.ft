assert_eq = func(expression, expected_result)
{
	if (expression == expected_result)
	{
		print("Test "+str(self.assert_eq.counter)+": Passed");
	}
	else
	{
		print("Test "+str(self.assert_eq.counter)+": Failed!");
		print("	evaluated to "+str(expression)+", expected: "+str(expected_result));
	}
	self.assert_eq.counter+=1;
}

assert_eq.counter = 0;
