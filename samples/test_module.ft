test_number = 0
assert_eq = func(expression, expected_result)
{
	if (expression == expected_result)
	{
		print("Test "+str(test_number)+": Passed");
	}
	else
	{
		print("Test "+str(test_number)+": Failed!");
		print("	evaluated to "+str(expression)+", expected: "+str(expected_result));
	}
	test_number+=1;
}
